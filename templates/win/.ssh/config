Host *
  ForwardAgent yes
  ServerAliveInterval 30
  ServerAliveCountMax 5
  GSSAPIAuthentication no

Host hoge
    HostName hoge
    Port 22
    ProxyCommand ssh -W %h:%p humidai1

Host github.com
  HostName github.com
  AddKeysToAgent yes
  User git
  IdentityFile C:\Users\{{ user_name }}\.ssh\{{ win_git_key_name }}

Host gitlab.com
  HostName gitlab.com
  AddKeysToAgent yes
  User git
  IdentityFile C:\Users\{{ user_name }}\.ssh\{{ win_git_key_name }}

Host *
  User {{ user_name }}
  IdentityFile C:\Users\{{ user_name }}\.ssh\{{ win_ssh_key_name }}

# 会社用
Host git.infrastructure.jp
  HostName git.infrastructure.jp
  AddKeysToAgent yes
  User git
  IdentityFile C:\Users\{{ user_name }}\.ssh\{{ win_git_key_name }}
